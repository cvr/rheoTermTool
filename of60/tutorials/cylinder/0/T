FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 462.0;

boundaryField
{
    inlet
    {
	type            fixedValue;
	value           uniform 462.0;
    }
    
    outlet
    {
    	type		zeroGradient;
    }

    walls
    {
	type            fixedValue;
	value           uniform 462.0;
    }
    
    cylinder
    {
    	type		zeroGradient;
    }
    
    frontAndBack
    {
        type           empty;
    }
    
}

