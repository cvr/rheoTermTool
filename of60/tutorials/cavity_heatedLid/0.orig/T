FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 450.0;

boundaryField
{
   
    movingLid
    {
	type            fixedValue;
	value           uniform 500.0;
    }
    
    bottomWall
    {
	type            fixedValue;
	value           uniform 400.0;
    }
    
    westWall
    {
    	type		zeroGradient;
    }
    
    eastWall
    {
    	type		zeroGradient;
    }
    frontAndBack
    {
    	type		empty;
    }
    
}

