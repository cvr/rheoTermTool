FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      mu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 1.5e-8;

boundaryField
{
    "(.*)"
    {
        type            zeroGradient;
    } 
}

